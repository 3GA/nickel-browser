+<script>
+function show() {
+  if (document.querySelector('#errorSummary > p > strong') == null) {
+    document.getElementById('classic').className = '';
+    document.getElementById('t').className = '';
+  } else {      
+    var url = document.querySelector('#errorSummary > p > strong').innerHTML.trim();
+    if (url.indexOf('my.jolicloud.com') != -1 ||
+        url.indexOf('dev.jolicloud.org') != -1) {
+      document.addEventListener('contextmenu', function(e) {
+        e.preventDefault();
+      });
+      document.getElementById('jolicloud').className = '';
+      document.getElementById('t').className = 'joli';
+    } else {
+      document.getElementById('classic').className = '';
+      document.getElementById('t').className = '';
+    }
+  }
+}
+function reload() {
+  var url = document.querySelector('#errorSummary > p > strong').innerHTML.trim();
+  window.location.href = url;
+}
+</script>
 
